alias s="cd .."
alias ll="ls -l"
alias la="ls -a"
alias lla="ls -al"
alias nn="netstat -tulpn"
alias cap="cap -f /etc/capistrano/Capfile"
alias c="clear"

alias whoiam="curl -s 169.254.169.254/latest/meta-data/iam/info"

alias kafka-logs='tail -f /mnt/log/kafka/server.log'
alias kafka-controller-logs='tail -f /mnt/log/kafka/controller.log'
